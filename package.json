{
  "name": "ultimate-quick-view-for-woocommerce",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:admin": "wp-scripts build src/admin.js --output-path=build/admin",
    "build:public": "wp-scripts build src/public.js --output-path=build/public",
    "build": "npm-run-all build:admin build:public",
    "start:admin": "wp-scripts start src/admin.js --webpack-src-dir=src/admin --output-path=build/admin",
    "start:public": "wp-scripts start src/public.js --output-path=build/public",
    "start": "npm-run-all --parallel start:admin start:public",
    "packages-update": "wp-scripts packages-update",
    "makejsstring": "node .bin/make-string.js",
    "makepot": "wp i18n make-pot . languages/wp-react-plugin-boilerplate.pot --skip-js --exclude=src,vendor,node_modules --merge=languages/js.pot",
    "copy": "node .bin/copy.js",
    "deploy": "npm-run-all build makepot copy",
    "initial-rename": "node .bin/initial-rename.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@wordpress/scripts": "^19.2.4",
    "@wordpress/babel-plugin-makepot": "^6.1.0",
    "autoprefixer": "^10.4.7",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "replace-in-file": "^7.2.0"
  },
  "dependencies": {
    "@wordpress/components": "^28.2.0",
    "@wordpress/element": "^6.2.0",
    "@wordpress/media-utils": "^5.2.0",
    "bootstrap": "^5.3.3",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.3",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.0",
    "@wordpress/url": "^4.1.0"
  }
}
