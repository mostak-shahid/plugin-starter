{
  "name": "plugin-starter",
  "version": "1.0.0",
  "description": "Plugin starter boilerplate for WordPress",
  "author": "Md. Mostak Shahid",
  "license": "GPL-2.0-or-later",
  "keywords": [],
  "scripts": {
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "format": "wp-scripts format",
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "makejsstring": "node .bin/make-string.js",
    "makepot": "wp i18n make-pot . languages/wp-react-plugin-boilerplate.pot --skip-js --exclude=src,vendor,node_modules --merge=languages/js.pot",
    "copy": "node .bin/copy.js",
    "deploy": "npm-run-all build makepot copy",
    "initial-rename": "node .bin/initial-rename.js"
  },
  "devDependencies": {
    "@wordpress/scripts": "^29.0.0",
    "@wordpress/babel-plugin-makepot": "^6.7.0",
    "autoprefixer": "^10.4.20",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "replace-in-file": "^7.2.0",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.1.3",
    "buffer": "^6.0.3",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "@woocommerce/dependency-extraction-webpack-plugin": "^3.0.1"
  },
  "dependencies": {
    "@wordpress/url": "^4.7.0",
    "@wordpress/components": "^28.7.0",
    "@wordpress/element": "^6.7.0",
    "@wordpress/media-utils": "^5.7.0",
    "bootstrap": "^5.3.3",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "react-select": "^5.8.0",
    "react-toastify": "^10.0.5"
  }
}
